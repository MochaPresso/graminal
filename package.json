  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.5.0",
    "cross-env": "^7.0.3",
    "electron": "^21.2.3",
    "electron-builder": "^23.6.0",
    "electron-rebuild": "^3.2.9",
    "wait-on": "^6.0.1"
  }
